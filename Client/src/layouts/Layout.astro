---
import Image from "astro/components/Image.astro";
import { Navbar } from "../components/navbar";
import { actions } from "astro:actions";

const c = await Astro.callAction(actions.carreras.get, {});
const carreras = c.data;
console.log('carreras:', carreras);

---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=DM+Sans:ital,opsz,wght@0,9..40,100..1000;1,9..40,100..1000&family=League+Gothic&display=swap" rel="stylesheet">
		<meta name="generator" content={Astro.generator} />
		<title>Astro Basics</title>
	</head>
	<body>
		<Navbar carreras={carreras} client:only/>
		<slot />
	</body>
</html>

